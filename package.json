{
  "name": "tsdown-preset-sxzz",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@10.30.3",
  "description": "An opinionated preset for tsdown.",
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sxzz",
  "homepage": "https://github.com/sxzz/tsdown-preset-sxzz#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/tsdown-preset-sxzz.git"
  },
  "bugs": {
    "url": "https://github.com/sxzz/tsdown-preset-sxzz/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsgo --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "tsdown": "^0.21.0-0"
  },
  "dependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "publint": "^0.3.17"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.8.1",
    "@sxzz/prettier-config": "^2.3.1",
    "@types/node": "^25.3.2",
    "@typescript/native-preview": "7.0.0-dev.20260227.1",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.2",
    "prettier": "^3.8.1",
    "tsdown": "^0.21.0-beta.2",
    "typescript": "^5.9.3"
  },
  "prettier": "@sxzz/prettier-config"
}
